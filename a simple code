#include <iostream>
#include <vector>

using namespace std;

int pmChecker(int a)
{
   int counter =0;
   for(int i=2;i<a;i++)
   {  
      if(a%i==0)
      {counter++;}
   }
   if(counter==2)
   {return 1;}
   else return 0;
}



int main(){
    
    int n;

cin >> n;
int a[n];

for(int i=0;i<n;i++)
{
    cin >> a[i];
}


for(int i=0;i<n;i++)
{
    int k=0;
    vector<int> b;
    int m = a[i]/2;
    for (int i = 1; i <= m; i++) 
    {
        b.push_back(i);
        if(pmChecker(*i)==1)
        {
          if(pmChecker(n-*i==1))
             {return k=1;}
        }
        if(k==1)
          {cout<<"YES"<<endl;}
    }
    
}


}

